{
  "kind": "collectionType",
  "collectionName": "codes",
  "info": {
    "name": "code"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "date": {
      "type": "date"
    },
    "time": {
      "type": "time"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "detail": {
      "type": "richtext"
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "codes"
    },
    "subtitle": {
      "type": "string"
    }
  }
